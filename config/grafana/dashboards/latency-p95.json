{
  "dashboard": {
    "title": "TogetherOS Latency & Performance",
    "tags": ["togetheros", "latency", "performance", "slo"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(togetheros_deployment_timestamp[1m]) > 0",
          "tagKeys": "version",
          "titleFormat": "Deployment"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 16, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\"}[5m])) by (le)) * 1000",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.90, sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\"}[5m])) by (le)) * 1000",
            "legendFormat": "p90"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\"}[5m])) by (le)) * 1000",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\"}[5m])) by (le)) * 1000",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 300 },
                { "color": "orange", "value": 500 },
                { "color": "red", "value": 1000 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "p95 Latency",
        "description": "SLO Target: <500ms",
        "type": "gauge",
        "gridPos": { "h": 8, "w": 4, "x": 16, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\"}[5m])) by (le)) * 1000",
            "legendFormat": "p95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0,
            "max": 1000,
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 300 },
                { "color": "orange", "value": 500 },
                { "color": "red", "value": 750 }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "SLO Compliance (p95 < 500ms)",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\", le=\"0.5\"}[1h])) / sum(rate(http_request_duration_seconds_count{job=\"togetheros-app\"}[1h]))",
            "legendFormat": "% under 500ms"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 2,
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 0.9 },
                { "color": "yellow", "value": 0.95 },
                { "color": "green", "value": 0.99 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Avg Response Time",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 20, "y": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_sum{job=\"togetheros-app\"}[5m])) / sum(rate(http_request_duration_seconds_count{job=\"togetheros-app\"}[5m])) * 1000",
            "legendFormat": "Average"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 200 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Latency by Route (p95)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "topk(5, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\"}[5m])) by (route, le))) * 1000",
            "legendFormat": "{{route}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        }
      },
      {
        "id": 6,
        "title": "Slowest Routes (p95)",
        "type": "table",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\"}[1h])) by (route, le))) * 1000",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": { "Time": true, "le": true },
              "renameByName": { "route": "Route", "Value": "p95 (ms)" }
            }
          }
        ]
      },
      {
        "id": 7,
        "title": "Request Rate",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"togetheros-app\"}[1m])) * 60",
            "legendFormat": "Requests/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqpm"
          }
        }
      },
      {
        "id": 8,
        "title": "Regression Detection",
        "description": "Routes with latency regression vs baseline",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "togetheros_route_latency_p95 / togetheros_route_latency_baseline_p95",
            "legendFormat": "{{route}} (ratio)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1.3 },
                { "color": "red", "value": 1.5 }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Apdex Score",
        "description": "Target: 0.95 (T=200ms, F=800ms)",
        "type": "gauge",
        "gridPos": { "h": 6, "w": 8, "x": 0, "y": 22 },
        "targets": [
          {
            "expr": "(sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\", le=\"0.2\"}[5m])) + sum(rate(http_request_duration_seconds_bucket{job=\"togetheros-app\", le=\"0.8\"}[5m])) / 2) / sum(rate(http_request_duration_seconds_count{job=\"togetheros-app\"}[5m]))",
            "legendFormat": "Apdex"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "decimals": 3,
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 0.7 },
                { "color": "yellow", "value": 0.85 },
                { "color": "green", "value": 0.95 }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Database Query Time",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 8, "x": 8, "y": 22 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket{job=\"togetheros-app\"}[5m])) by (le)) * 1000",
            "legendFormat": "p95"
          },
          {
            "expr": "sum(rate(db_query_duration_seconds_sum{job=\"togetheros-app\"}[5m])) / sum(rate(db_query_duration_seconds_count{job=\"togetheros-app\"}[5m])) * 1000",
            "legendFormat": "avg"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        }
      },
      {
        "id": 11,
        "title": "Event Loop Lag",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 8, "x": 16, "y": 22 },
        "targets": [
          {
            "expr": "nodejs_eventloop_lag_seconds{job=\"togetheros-app\"} * 1000",
            "legendFormat": "Event Loop Lag"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 50 },
                { "color": "red", "value": 100 }
              ]
            }
          }
        }
      }
    ]
  }
}
